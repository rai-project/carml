{
  "name": "mlmodelscope",
  "version": "0.4.10",
  "private": true,
  "config-overrides-path": "/config-overrides.js",
  "dependencies": {
    "@uppy/react": "^1.3.2",
    "node-sass": "^4.12.0",
    "react": "^16.10.2",
    "react-app-rewired": "^2.1.3",
    "react-compare-image": "^3.1.0",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "rxjs": "^7.5.2",
    "typeface-lato": "0.0.75",
    "typeface-source-code-pro": "0.0.71",
    "uppy": "^1.5.2"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "copy:redirect": "cp ./_redirects build",
    "test": "react-app-rewired test --env=jsdom --setupFilesAfterEnv='./src/setupTests.js'",
    "eject": "react-scripts eject",
    "bundle": "npm-run-all clean:build build build:assets",
    "clean:build": "rimraf build",
    "build:assets": "node scripts/build_go_assets.js",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.16.7",
    "@storybook/addon-actions": "^6.4.14",
    "@storybook/addon-essentials": "^6.4.14",
    "@storybook/addon-links": "^6.4.14",
    "@storybook/node-logger": "^6.4.14",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.14",
    "@svgr/webpack": "^4.3.3",
    "babel-plugin-import": "^1.12.2",
    "customize-cra": "^0.8.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint-config-prettier": "^6.4.0",
    "expect": "^27.5.1",
    "fetch-mock": "^9.11.0",
    "lint-staged": "^9.4.2",
    "npm-run-all": "^4.1.5",
    "raw-loader": "^3.1.0",
    "react-error-overlay": "6.0.9",
    "rimraf": "^3.0.0"
  },
  "resolutions": {
    "//": "See https://github.com/facebook/create-react-app/issues/11773",
    "react-error-overlay": "6.0.9"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --parser flow --write",
      "git add"
    ],
    "src/**/*.css": [
      "stylefmt",
      "stylelint scss",
      "git add"
    ],
    "src/**/*.scss": [
      "stylefmt",
      "stylelint --syntax scss",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
